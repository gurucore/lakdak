{
  "name": "lakdak",
  "version": "0.0.10",
  "keywords": [
    "node",
    "nodejs",
    "lockevn"
  ],
  "description": "Library for common utils in NodeJS to develop backend web API",
  "releasenote": "Setup project to build and output ES and UMD module. Importable to XC-API",
  "author": "lockevn",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=16.x"
  },
  "files": [
    "dist"
  ],
  "source": "src/index.ts",
  "main": "./dist/lakdak.umd.js",
  "module": "./dist/lakdak.es.js",
  "exports": {
    ".": {
      "import": "./dist/lakdak.es.js",
      "require": "./dist/lakdak.umd.js"
    }
  },
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "dev": "parcel watch",
    "prebuild": "rimraf dist",
    "build": "parcel build",
    "lint": "eslint . --fix",
    "check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "ci": "pnpm build && pnpm test && pnpm lint && pnpm check"
  },
  "devDependencies": {
    "@parcel/packager-ts": "2.8.2",
    "@parcel/transformer-typescript-tsc": "^2.8.2",
    "@parcel/transformer-typescript-types": "2.8.2",
    "@types/node": "^16.18.3",
    "@vitest/ui": "^0.24.5",
    "husky": "^8.0.3",
    "parcel": "latest",
    "rimraf": "latest",
    "typescript": "~4.3.5",
    "vite": "^3.2.3",
    "vitest": "^0.24.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm build && pnpm test:run"
    }
  },
  "stackblitz": {
    "startCommand": "pnpm test:ui"
  }
}
