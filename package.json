{
  "name": "lakdak",
  "version": "0.0.3",
  "keywords": [
    "node",
    "nodejs",
    "lockevn"
  ],
  "description": "Library for common utils in NodeJS to develop backend web API",
  "releasenote": "Setup project to build and output ES and UMD module. Importable to XC-API",
  "author": "lockevn",
  "license": "Copyright",
  "engines": {
    "node": "16.x"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/lakdak.umd.js",
  "module": "./dist/lakdak.es.js",
  "exports": {
    ".": {
      "import": "./dist/lakdak.es.js",
      "require": "./dist/lakdak.umd.js"
    }
  },
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preserve": "vite build",
    "serve": "vite preview --port 5050",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/node": "^16.18.3",
    "@vitest/ui": "^0.24.5",
    "typescript": "~4.3.5",
    "vite": "^3.2.3",
    "vitest": "^0.24.5"
  },
  "stackblitz": {
    "startCommand": "npm run test:ui"
  }
}
